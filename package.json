{
  "name": "lexbot",
  "version": "3.0.0",
  "description": "Bot for Discord.js v14",
  "main": "src/bot.js",
  "scripts": {
    "prebuild": "node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "tsc",
    "start": "node dist/bot.js",
    "debug": "nodemon src/bot.ts"
  },
